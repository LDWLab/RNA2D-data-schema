{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Nucleotide object",
    "description": "Secondary structure of nucleotides.",
    "id": "nucleotide.json#",
    "type": "object",
    "required": ["residueIndex", "residueName", "x", "y", "classes"],
    "additionalProperties": false,
    "properties": {
        "residueIndex": {
            "type": "integer"
        },
        "residueName": {
            "type": "string",
            "enum": ["A", "C", "G", "U"]
        },
        "x": {
            "type": "number"
        },
        "y": {
            "type": "number"
        },
        "classes": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}